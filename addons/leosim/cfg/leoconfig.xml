		<dxl_torqueconstant>0.00992</dxl_torqueconstant>
		<dxl_terminalresistance>8.6</dxl_terminalresistance>
		<dxl_gearboxratio>193</dxl_gearboxratio>
		<dxl_gearboxefficiency>0.75</dxl_gearboxefficiency>
