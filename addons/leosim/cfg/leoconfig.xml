		<degtorad>pi/180.0</degtorad>
		<dxlheight>0.051</dxlheight>
		<dxlwidth>0.036</dxlwidth>
		<dxldepth>0.036</dxldepth>
		<torsowidth>0.160</torsowidth>
		<torsoheight>0.24155</torsoheight>
