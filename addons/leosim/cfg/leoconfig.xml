		<shoulderoffsety>0.11975</shoulderoffsety>
		<upleglength>0.116</upleglength>
		<loleglength>0.1045</loleglength>
		<interlegdist>0.06390</interlegdist>
		<footlength>0.081</footlength>
		<footwheelradius>0.013</footwheelradius>
		<footwheelringradius>0.0035</footwheelringradius>
		<footgeomlength>0.5</footgeomlength>
		<foambumperlength>0.5</foambumperlength>
		<kneewheeloffsetx>0.031</kneewheeloffsetx>
		<kneewheeloffsetz>0.0165</kneewheeloffsetz>
		<inertiadontcare>0.001</inertiadontcare>
    <inertiainf>1000000</inertiainf>
		<torsoHipDistX>0.00273</torsoHipDistX>>
		<torsoHipDistZ>-torsoheight/2</torsoHipDistZ>>
		<!-- specific mass and inertia constants that are needed later -->
		<!-- raw torso properties, excluding boom influences -->
		<torsoMass>0.91326</torsoMass>
		<torsoCMX>-0.00102</torsoCMX>
		<torsoCMZ>0.009945</torsoCMZ>
		<torsoIYY>0.004676374</torsoIYY>
		<!-- boom characteristics -->
		<boomMass>0.860</boomMass>
		<boomCMY>0.835</boomCMY>
		<boomLength comment="Distance between central axis/tube and middle of robot">1.70</boomLength>
		<boomIZZ>0.31863</boomIZZ>
		<boomVirtualMassX>(boomCMY^2*boomMass + boomIZZ)/(boomLength^2)</boomVirtualMassX>
		<boomVirtualMassZ>boomMass*boomCMY/boomLength</boomVirtualMassZ>
		<boomExtForce>(boomVirtualMassZ - boomVirtualMassX)*(-9.81)</boomExtForce>
		<!-- torso with boom effects -->
		<torsoBoomMass>torsoMass + boomVirtualMassX</torsoBoomMass>
		<torsoBoomCMX>(torsoMass*torsoCMX + boomVirtualMassX*torsoHipDistX)/(torsoMass + boomVirtualMassX)</torsoBoomCMX>
		<torsoBoomCMZ>(torsoMass*torsoCMZ + boomVirtualMassX*torsoHipDistZ)/(torsoMass + boomVirtualMassX)</torsoBoomCMZ>
		<torsoBoomIYY>torsoIYY + torsoMass*((torsoCMX-torsoBoomCMX)^2 + (torsoCMZ-torsoBoomCMZ)^2) + boomVirtualMassX*((torsoHipDistX-torsoBoomCMX)^2 + (torsoHipDistZ-torsoBoomCMZ)^2)</torsoBoomIYY>
		<!-- material constants -->
		<rubber>0</rubber>
		<foam>1</foam>
		<!-- Joint settings -->
		<mintorsoangle>-pi</mintorsoangle>
		<maxtorsoangle>pi</maxtorsoangle>
		<minhipangle>-0.785398163397448</minhipangle>
		<maxhipangle>2.548180707911721</maxhipangle>
		<minkneeangle>-2.617993877991494</minkneeangle>
		<maxkneeangle>0.034906585039887</maxkneeangle>
		<minankleangle>-1.570796326794897</minankleangle>
		<maxankleangle>1.448623279155294</maxankleangle>
		<hipstopK>100</hipstopK>
		<hipstopD>10</hipstopD>
		<kneestopK>50</kneestopK>
		<kneestopD>5</kneestopD>
		<anklestopK>100</anklestopK>
		<anklestopD>10</anklestopD>
		<!-- Motor settings -->
